{
  "name": "@blackglory/git-list",
  "version": "0.2.4",
  "description": "A tool for batch operation of git repositories",
  "keywords": [],
  "bin": {
    "git-list": "dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "repository": "git@github.com:BlackGlory/git-list.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "deduplicate": "yarn-deduplicate",
    "prepublishOnly": "run-s clean build bundle",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js",
    "dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node -r tsconfig-paths/register src/cli.ts",
    "clean": "rimraf lib dist",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "bundle": "webpack --stats-error-details",
    "smoke": "node dist/cli.cjs --help",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint build test bundle smoke",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/git-url-parse": "^9.0.1",
    "@types/jest": "^29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-shebang-plugin": "^1.1.8",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/errors": "^2.3.0",
    "@blackglory/types": "^1.4.0",
    "ajv": "^8.11.2",
    "commander": "^9.4.1",
    "extra-filesystem": "^0.4.8",
    "extra-promise": "^4.4.0",
    "extra-retry": "^0.3.5",
    "extra-tags": "^0.3.0",
    "git-url-parse": "^11.6.0",
    "iterable-operator": "^2.5.0",
    "js-yaml": "^4.1.0",
    "simple-git": "^2.48.0"
  }
}
