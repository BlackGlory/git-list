{
  "name": "@blackglory/git-list",
  "version": "0.3.0",
  "description": "A tool for batch operation of git repositories",
  "keywords": [],
  "type": "module",
  "bin": {
    "git-list": "lib/cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/git-list.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "vitest --run",
    "prepublishOnly": "run-s clean prepare build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "smoke": "node lib/cli.js --help",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean prepare lint build test smoke",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/git-url-parse": "^9.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-patch": "^2.1.0",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/errors": "^3.0.0",
    "@blackglory/prelude": "^0.3.1",
    "ajv": "^8.12.0",
    "commander": "^10.0.0",
    "extra-filesystem": "^0.5.2",
    "extra-promise": "^6.0.3",
    "extra-retry": "^0.4.2",
    "extra-tags": "^0.3.1",
    "git-url-parse": "^13.1.0",
    "iterable-operator": "^4.0.3",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.16.0"
  }
}
