{
  "name": "@blackglory/git-list",
  "version": "0.1.7",
  "description": "A tool for batch operation of git repositories",
  "keywords": [],
  "bin": {
    "git-list": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/git-list.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js",
    "dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node -r tsconfig-paths/register src/cli.ts",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "start": "serve dist",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/git-url-parse": "^9.0.1",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@blackglory/errors": "^1.1.2",
    "@blackglory/types": "^0.6.4",
    "ajv": "^8.8.2",
    "commander": "^8.3.0",
    "extra-filesystem": "^0.4.1",
    "extra-promise": "^0.21.0",
    "extra-retry": "^0.3.1",
    "extra-tags": "^0.2.4",
    "git-url-parse": "^11.6.0",
    "iterable-operator": "^0.14.4",
    "js-yaml": "^4.1.0",
    "simple-git": "^2.48.0"
  }
}
